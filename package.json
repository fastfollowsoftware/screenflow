{
  "name": "screenflow",
  "displayName": "Screenflow",
  "description": "Visual wireframe preview for text-based screen definitions",
  "version": "0.1.0",
  "publisher": "fastfollow",
  "repository": {
    "type": "git",
    "url": "https://github.com/fastfollowsoftware/screenflow"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "screenflow.showPreview",
        "title": "Show Screenflow Preview",
        "icon": "$(preview)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceFilename == screens.txt || resourceExtname == .screenflow",
          "command": "screenflow.showPreview",
          "group": "navigation"
        }
      ]
    },
    "languages": [
      {
        "id": "screenflow",
        "extensions": [
          ".screenflow"
        ],
        "aliases": [
          "Screenflow",
          "screenflow"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
